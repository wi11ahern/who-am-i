{
  "family": "willahern_com",
  "networkMode": "bridge",
  "containerDefinitions": [
      {
          "name": "react",
          "image": "927822646792.dkr.ecr.us-east-1.amazonaws.com/willahern-com-ecr-dev:latest",
          "cpu": "0.25vCPU",
          "memory": "0.5GB",
          "portMappings": [
              {
                  "containerPort": 3000,
                  "hostPort": 3000,
                  "protocol": "http"
              }
          ],
          "essential": true,
          "privileged": true,
      }
  ],
  "cpu": 512,
  "memory": 300,
  "tags": [
      {
          "key": "Environment",
          "value": "dev"
      }
  ],
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  }
}